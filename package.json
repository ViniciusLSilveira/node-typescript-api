{
    "name": "typescript-api",
    "version": "1.0.0",
    "description": "Typescript Node.js API",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/src/index.js",
        "start:dev": "ts-node-dev --transpile-only src/index.ts",
        "test": "yarn test:unit && yarn test:functional",
        "test:functional": "jest --projects ./test --forceExit --runInBand",
        "test:unit": "jest",
        "lint": "eslint ./src ./test --ext .ts",
        "lint:fix": "eslint ./src ./test --ext .ts --fix",
        "style:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
        "style:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
    },
    "engines": {
        "node": ">=14"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ViniciusLSilveira/node-typescript-api.git"
    },
    "keywords": [
        "node",
        "nodejs",
        "node.js",
        "typescript",
        "api"
    ],
    "author": "Vinicius Lima",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ViniciusLSilveira/node-typescript-api/issues"
    },
    "homepage": "https://github.com/ViniciusLSilveira/node-typescript-api#readme",
    "dependencies": {
        "@overnightjs/core": "^1.7.6",
        "axios": "^0.26.1",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.20.0",
        "config": "^3.3.7",
        "express": "^4.17.3",
        "jsonwebtoken": "^8.5.1",
        "module-alias": "^2.2.2",
        "mongoose": "^6.3.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/config": "^0.0.41",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.1",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/module-alias": "^2.0.1",
        "@types/node": "^17.0.24",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.19.0",
        "@typescript-eslint/parser": "^5.19.0",
        "eslint": "^8.13.0",
        "jest": "^27.5.1",
        "nock": "^13.2.4",
        "prettier": "^2.6.2",
        "supertest": "^6.2.2",
        "ts-jest": "^27.1.4",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.6.3"
    }
}
