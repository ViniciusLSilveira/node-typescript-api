{
    "name": "typescript-api",
    "version": "1.0.0",
    "description": "Typescript Node.js API",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "start": "yarn build && node dist/src/index.js",
        "start:dev": "ts-node-dev src/index.ts",
        "test": "echo \"Error: no test specified\" && exit 1",
        "test:functional": "jest --projects ./test --runInBand",
        "lint": "eslint ./src ./test --ext .ts",
        "lint:fix": "eslint ./src ./test --ext .ts --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ViniciusLSilveira/node-typescript-api.git"
    },
    "keywords": [
        "node.js",
        "typescript",
        "api"
    ],
    "author": "Vinicius Lima",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ViniciusLSilveira/node-typescript-api/issues"
    },
    "homepage": "https://github.com/ViniciusLSilveira/node-typescript-api#readme",
    "devDependencies": {
        "@types/jest": "^27.4.1",
        "@types/module-alias": "^2.0.1",
        "@types/node": "^17.0.23",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.18.0",
        "@typescript-eslint/parser": "^5.18.0",
        "eslint": "^8.12.0",
        "jest": "^27.5.1",
        "supertest": "^6.2.2",
        "ts-jest": "^27.1.4",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.6.3"
    },
    "dependencies": {
        "module-alias": "^2.2.2"
    }
}
